<div class="container">
  <h1>Lists</h1>
  <%= link_to "Add", new_list_path, class: 'btn btn-primary my-3' %>

  <% @lists.each do |list| %>
  <div>
    <%= link_to list_path(list['id']) do %>
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(cl_image_tag(list.photo.key))">
          <%= cl_image_tag(list.photo.key, width: 400, height: 300, crop: :fill) %>
          <%= list.name %>
      </div>
    <% end %>
  </div>
  <% end %>
</div>
